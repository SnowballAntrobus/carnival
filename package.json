{
  "name": "carnival",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/SnowballAntrobus/carnival.git",
  "author": "Dante Ramón Gil-Marín <drgilmarin@uchicago.edu>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0"
  },
  "workspaces": {
    "packages": [
      "@app/*"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "proseWrap": "always",
    "overrides": [
      {
        "files": [
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "printWidth": 120
        }
      }
    ]
  }
}
