{
  "name": "carnival",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/SnowballAntrobus/carnival.git",
  "author": "Dante Ramón Gil-Marín <drgilmarin@uchicago.edu>",
  "license": "MIT",
  "scripts": {
    "setup:env": "node ./scripts/setup_env.js",
    "setup:db": "node ./scripts/setup_db.js",
    "clean": "node ./scripts/clean.js",
    "reset": "yarn clean && node ./scripts/delete-env-file.js",
    "--shortcuts to run commands in workspaces--": "",
    "server": "yarn workspace @app/server",
    "db": "yarn workspace @app/db"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "typescript": "^4.4.4",
    "dotenv": "^10.0.0",
    "inquirer": "^8.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "@types/node": "^15.6.1",
    "ts-node": "^10.0.0",
    "rimraf": "^3.0.2",
    "update-dotenv": "^1.1.1"
  },
  "resolutions": {
    "graphql": "15.x",
    "pg-connection-string": "2.x"
  },
  "workspaces": {
    "packages": [
      "@app/*"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "proseWrap": "always",
    "overrides": [
      {
        "files": [
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "printWidth": 120
        }
      }
    ]
  }
}
